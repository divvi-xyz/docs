{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "가이드",
        "groups": [
          {
            "group": "시작하기",
            "pages": ["index"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Checkable 체커블",
          "href": "https://checkable.app",
          "icon": "book-open-cover"
        },
        {
          "anchor": "체커블 Discord 커뮤니티",
          "href": "https://discord.gg/x8Qtt2aasJ",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:help@checkable.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://www.checkable.app/workspaces"
    }
  },
  "fonts": {
    "family": "Pretendard",
    "weight": 400,
    "source": "https://fastly.jsdelivr.net/gh/Project-Noonnu/noonfonts_2107@1.1/Pretendard-Regular.woff",
    "format": "woff",
    "heading": {
      "family": "Pretendard",
      "weight": 900,
      "source": "https://fastly.jsdelivr.net/gh/Project-Noonnu/noonfonts_2107@1.1/Pretendard-Black.woff",
      "format": "woff"
    }
  }
}
