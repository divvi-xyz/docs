{
  "name": "divvi-docs-v2",
  "version": "1.0.0",
  "private": true,
  "description": "Divvi documentation website",
  "scripts": {
    "generate-docs": "ts-node scripts/generate-docs.ts",
    "generate-docs:clean": "ts-node scripts/generate-docs.ts --clean",
    "generate-docs:watch": "nodemon --exec 'yarn generate-docs' --watch docs-template --watch submodules --ext md,mdx,json,yml,yaml,ts,tsx,js,jsx,html,css,png,jpg,jpeg,gif,svg,mp4,mov,avi --ignore node_modules --ignore .git --delay 250ms",
    "dev": "concurrently \"yarn generate-docs:watch\" \"cd docs-generated && npx mint dev\""
  },
  "devDependencies": {
    "@mintlify/models": "^0.0.216",
    "@types/node": "^22.0.0",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.2.0",
    "mint": "^4.2.68",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
